
HOMEPAGE="http://audacious-media-player.org/"
SRC_URI="http://distfiles.atheme.org/${P}.tbz2"

NO_AUTOPOINT=1
NO_ACLOCAL=1

src_compile() {
	cd ${S}
	cygautoreconf
	lndirs
	cd ${B}
	cygconf \
		--enable-dbus \
		--disable-samplerate \
		--disable-altivec \
		--disable-chardet \
		--with-regexlib=gnu
	cygmake
}
